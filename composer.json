{
	"name": "escuelademusica/laravel-mjml",
	"authors": [
		{
			"name": "Malico",
			"email": "malico@escuelademusica.com"
		}
	],
	"autoload": {
		"psr-4": {
			"EscuelaDeMusica\\MJML\\": "src/"
		},
		"files": [
			"src/Support/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"EscuelaDeMusica\\MJML\\MjmlServiceProvider"
			]
		}
	},
	"require-dev": {
		"orchestra/testbench": "^6.22"
	},
	"require": {
		"illuminate/support": "*",
		"illuminate/view": "*"
	},
	"archive": {
		"exclude": [
			"tests/",
			"*.xml"
		]
	},
	"license": "MIT"
}
